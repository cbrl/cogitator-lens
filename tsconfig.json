{
    "compilerOptions": {
        "module": "Node16",
        "target": "ES2022",
        "lib": [
            "ES2022"
        ],
        "sourceMap": true,
        "rootDir": "src",
        "strict": true,
		"alwaysStrict": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        //"noUnusedLocals": true,
        //"noUnusedParameters": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true
    },
    "exclude": [
        "node_modules",
        ".vscode-test"
    ]
}